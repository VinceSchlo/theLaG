{
  "name": "thelag",
  "version": "0.0.1",
  "description": "Rework of the-lag.com",
  "main": "index.js",
  "scripts": {
    "css-build": "node-sass app/src/scss/main.scss build/css/main.min.css --output-style compressed",
    "css-watch": "npm run css-build -- --watch",
    "js-watch": "babel app/src/js/main.js --no-babelrc --watch --out-file build/js/main.js",
    "dev-css": "npm run css-build && npm run css-watch",
    "dev-js": "npm run js-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VinceSchlo/theLag.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VinceSchlo/theLag/issues"
  },
  "homepage": "https://github.com/VinceSchlo/theLag#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "node-sass": "^4.11.0"
  }
}
